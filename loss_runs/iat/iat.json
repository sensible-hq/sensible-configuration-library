{
  "fingerprint": {
    "tests": [
      {
        "text": "Loss Run Report",
        "type": "equals",
        "isCaseSensitive": true
      },
      {
        "text": "IAT Group",
        "type": "equals",
        "isCaseSensitive": true
      },
      {
        "text": "Historical Period:",
        "type": "startsWith",
        "isCaseSensitive": true
      },
      {
        "text": "Unit:",
        "type": "equals",
        "isCaseSensitive": true
      }
    ]
  },
  "preprocessors": [
    {
      "type": "ocr",
      "match": "page: 1",
      "matchAll": true,
      "pageOffset": 0
    }
  ],
  "fields": [
    {
      "id": "loss_run_policy",
      "type": "sections",
      "range": {
        "anchor": {
          "match": {
            "text": "Policy Number:",
            "type": "equals"
          }
        },
        "stop": {
          "type": "any",
          "matches": [
            {
              "type": "equals",
              "text": "TOTALS:",
              "isCaseSensitive": true
            },
            {
              "type": "startsWith",
              "text": "No claims found"
            }
          ]
        },
        "requireStop": true
      },
      "fields": [
        {
          "id": "insured",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "first"
          },
          "anchor": {
            "match": {
              "text": "insured:",
              "type": "startsWith"
            }
          }
        },
        {
          "id": "policy_number",
          "method": {
            "id": "label",
            "position": "above"
          },
          "anchor": {
            "match": {
              "type": "regex",
              "pattern": "[A-Z]{2}/[\\d]+"
            }
          }
        },
        {
          "id": "policy_start_date",
          "type": "date",
          "method": {
            "id": "label",
            "position": "right",
            "tiebreaker": "<"
          },
          "anchor": {
            "match": {
              "text": "Historical Period:",
              "type": "startsWith"
            }
          }
        },
        {
          "id": "policy_start_date",
          "type": "date",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "first"
          },
          "anchor": {
            "match": {
              "text": "Historical Period:",
              "type": "startsWith"
            }
          }
        },
        {
          "id": "policy_end_date",
          "type": "date",
          "method": {
            "id": "label",
            "position": "right",
            "tiebreaker": ">"
          },
          "anchor": {
            "match": {
              "text": "Historical Period:",
              "type": "startsWith"
            }
          }
        },
        {
          "id": "policy_end_date",
          "type": "date",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "second"
          },
          "anchor": {
            "match": {
              "text": "Historical Period:",
              "type": "startsWith"
            }
          }
        },
        {
          "id": "open_claims",
          "type": "number",
          "method": {
            "id": "passthrough"
          },
          "anchor": "not configured, not found"
        },
        {
          "id": "closed_claims",
          "type": "number",
          "method": {
            "id": "passthrough"
          },
          "anchor": "not configured, not found"
