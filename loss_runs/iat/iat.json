              "anchor": {
                "match": {
                  "type": "regex",
                  "pattern": "^\\d{12}"
                }
              }
            },
            {
              "id": "claim_type",
              "method": {
                "id": "split",
                "source_id": "_loss_ded_raw",
                "separator": "/",
                "index": 0
              }
            },
            {
              "id": "hide_fields",
              "method": {
                "id": "suppressOutput",
                "source_ids": [
                  "_loss_ded_raw"
                ]
              }
            },
