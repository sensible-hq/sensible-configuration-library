              "type": "date",
              "method": {
                "id": "row",
                "position": "right",
                "tiebreaker": "first"
              },
              "anchor": {
                "match": {
                  "type": "regex",
                  "pattern": "^[A-Z]{3}\\d{7}$"
                }
              }
