{
  "fingerprint": {
    "tests": [
      {
        "text": "Name:",
        "type": "equals",
        "isCaseSensitive": true
      },
      {
        "text": "Account Number:",
        "type": "equals",
        "isCaseSensitive": true
      },
      {
        "text": "Valued as of",
        "type": "startsWith",
        "isCaseSensitive": true
      },
      {
        "text": "Account Total",
        "type": "equals",
        "isCaseSensitive": true
      }
    ]
  },
  "fields": [
    {
      "id": "_insured",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "text": "name:",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "loss_run_policy",
      "type": "sections",
      "range": {
        "anchor": {
          "match": {
            "pattern": "^[\\d]{7}-\\d$",
            "type": "regex"
          }
        },
        "stop": {
          "type": "any",
          "matches": [
            {
              "pattern": "^[\\d]{7}-\\d Total",
              "type": "regex"
            }
          ]
        },
        "requireStop": true
      },
      "fields": [
        {
          "id": "insured",
          "method": {
            "id": "copy_to_section",
            "source_id": "_insured"
          }
        },
        {
          "id": "policy_number",
          "method": {
            "id": "passthrough"
          },
          "anchor": {
            "match": {
              "pattern": "^[\\d]{7}-",
              "type": "regex"
            },
            "end": "$"
          }
        },
        {
          "id": "policy_start_date",
          "type": "date",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "first"
          },
          "anchor": {
            "match": {
              "pattern": "^[\\d]{7}-",
              "type": "regex"
            },
            "end": "$"
          }
        },
        {
          "id": "policy_end_date",
          "type": "date",
          "method": {
            "id": "region",
            "start": "left",
            "width": 0.7,
            "height": 0.2,
            "offsetX": 0.7,
            "offsetY": 0.1
          },
          "anchor": {
            "match": {
              "pattern": "^[\\d]{7}-",
              "type": "regex"
            },
            "end": "$"
          }
        },
        {
          "id": "open_claims",
          "type": "number",
          "method": {
            "id": "passthrough"
          },
          "anchor": "not configured, not found"
        },
        {
          "id": "closed_claims",
          "type": "number",
          "method": {
            "id": "passthrough"
          },
          "anchor": "not configured, not found"
        },
        {
          "id": "total_claims",
          "type": "number",
          "method": {
            "id": "passthrough"
          },
          "anchor": "not configured, not found"
        },
        {
          "id": "claims_details",
          "type": "sections",
          "range": {
            "anchor": {
              "start": {
                "pattern": "^[\\d]{7}-",
                "type": "regex"
              },
              "match": {
                "type": "regex",
                "pattern": "[A-Z]{2}[\\d]{7}$"
              }
            },
            "stop": {
              "type": "endsWith",
              "text": "total"
            }
          },
          "fields": [
            {
              "id": "claimant",
              "method": {
                "id": "passthrough"
              },
              "anchor": "not found, not configured"
            },
            {
              "id": "claim_number",
              "type": {
                "id": "custom",
                "pattern": "[A-Z]{2}[\\d]{7}"
              },
              "method": {
                "id": "passthrough"
              },
              "anchor": {
                "match": {
                  "type": "regex",
                  "pattern": "[A-Z]{2}[\\d]{7}$"
                }
              }
            },
            {
              "id": "claim_type",
              "method": {
                "id": "row",
                "position": "right",
                "tiebreaker": "second",
                "wordFilters": [
                  "close",
                  "open"
                ]
              },
              "anchor": {
                "match": {
                  "type": "regex",
                  "pattern": "[A-Z]{2}[\\d]{7}$"
                }
              }
            },
            {
              "id": "_claim_status_raw",
              "method": {
                "id": "row",
                "position": "right",
                "tiebreaker": "second"
              },
              "anchor": {
                "match": {
                  "type": "regex",
                  "pattern": "[A-Z]{2}[\\d]{7}$"
                }
              }
            },
            {
              "id": "claim_status",
              "method": {
                "id": "split",
                "source_id": "_claim_status_raw",
                "separator": " ",
                "index": 0
