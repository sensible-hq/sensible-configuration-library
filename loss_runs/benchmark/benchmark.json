{
  "fingerprint": {
    "tests": [
      {
        "page": "first",
        "match": [
          {
            "text": "Cumulative Loss Run Report Criteria",
            "type": "startsWith",
            "isCaseSensitive": true
          },
          {
            "text": "benchmark insurance",
            "type": "includes"
          },
          {
            "text": "policy period range",
            "type": "includes"
          },
          {
            "text": "accident period range",
            "type": "includes"
          }
        ]
      },
      {
        "page": "last",
        "match": [
          {
            "text": "Grand Total Summary",
            "type": "includes",
            "isCaseSensitive": true
          },
          {
            "text": "Grand Totals",
            "type": "endsWith",
            "isCaseSensitive": true
          }
        ]
      }
    ]
  },
  "fields": [
    {
      "id": "loss_run_policy",
      "type": "sections",
      "range": {
        "anchor": {
          "start": {
            "text": "valued at",
            "type": "startsWith"
          },
          "match": {
            "text": "Claimant Name",
            "type": "startsWith"
          }
        },
        "offsetY": -1
      },
      "fields": [
        {
          "id": "insured",
          "method": {
            "id": "label",
            "position": "right"
          },
          "anchor": {
            "match": {
              "text": "insured",
              "type": "startsWith"
            }
          }
        },
        {
          "id": "policy_number",
          "method": {
            "id": "label",
            "position": "below"
          },
          "anchor": {
            "match": {
              "text": "policy number",
              "type": "startsWith"
            }
          }
        },
        {
          "id": "policy_start_date",
          "type": "date",
          "method": {
            "id": "label",
            "position": "below",
            "tiebreaker": "<"
          },
          "anchor": {
            "match": {
              "text": "policy effective dates",
              "type": "startsWith"
            }
          }
        },
        {
          "id": "policy_end_date",
          "type": "date",
          "method": {
            "id": "label",
            "position": "below",
            "tiebreaker": ">"
          },
          "anchor": {
            "match": {
              "text": "policy effective dates",
              "type": "startsWith"
            }
          }
        },
        {
          "id": "open_claims",
          "type": "number",
          "match": "last",
          "method": {
            "id": "label",
            "position": "right"
          },
          "anchor": {
            "start": {
              "text": "SUMMARY FOR",
              "type": "startsWith",
              "isCaseSensitive": true
            },
            "match": {
              "text": "open",
              "type": "startsWith"
            }
          }
        },
        {
          "id": "closed_claims",
          "type": "number",
          "match": "last",
          "method": {
            "id": "label",
            "position": "right"
          },
          "anchor": {
            "start": {
              "text": "SUMMARY FOR",
              "type": "startsWith",
              "isCaseSensitive": true
            },
            "match": {
              "text": "closed",
              "type": "startsWith"
            }
          }
        },
        {
          "id": "total_claims",
          "type": "number",
          "match": "last",
          "method": {
            "id": "label",
            "position": "right"
          },
          "anchor": {
            "start": {
              "text": "SUMMARY FOR",
              "type": "startsWith",
              "isCaseSensitive": true
            },
            "match": {
              "text": "total",
              "type": "startsWith"
            }
          }
        },
        {
          "id": "claims_details",
          "type": "sections",
          "range": {
            "anchor": {
              "start": {
                "type": "includes",
                "text": "claimnumber"
              },
              "match": {
                "type": "regex",
                "pattern": "^\\d{6,8}$"
              }
            }
          },
          "fields": [
            {
              "id": "claimant",
              "anchor": {
                "match": {
                  "type": "regex",
                  "pattern": "^\\d{6,8}$"
                }
              },
              "method": {
                "id": "row",
                "position": "right",
                "tiebreaker": "first"
              }
            },
            {
              "id": "claim_number",
              "method": {
                "id": "passthrough"
              },
              "anchor": {
                "match": {
                  "type": "regex",
                  "pattern": "^\\d{6,8}$"
                }
              }
            },
            {
              "id": "claim_type",
              "method": {
                "id": "region",
                "start": "left",
                "offsetX": -0.05,
                "offsetY": 0.4,
                "width": 1,
                "height": 0.2
              },
              "anchor": {
                "match": {
                  "type": "regex",
                  "pattern": "^\\d{6,8}$"
