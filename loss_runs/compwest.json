{
    "fingerprint": {
      "tests": [
        {
          "page": "any",
          "match": [
            {
              "text": "loss analysis",
              "type": "equals"
            },
            {
              "text": "account number:",
              "type": "startsWith"
            }
          ]
        }
      ]
    },
    "fields": [
      {
        "id": "policy_period",
        "method": {
          "id": "label",
          "position": "right"
        },
        "anchor": "policy effective dates"
      },
      {
        "id": "policy_premium",
        "method": {
          "id": "passthrough"
        },
        "anchor": "not configured, not available"
      },
      {
        "id": "policy_premium.year",
        "match": "all",
        "method": {
          "id": "regex",
          "pattern": "^20\\d{2}"
        },
        "anchor": {
          "start": [
            {
              "text": "five-year summary",
              "type": "startsWith"
            },
            {
              "text": "loss ratio",
              "type": "startsWith"
            }
          ],
          "match": {
            "pattern": "^20\\d{2}",
            "type": "regex"
          },
          "end": {
            "text": "totals:",
            "type": "startsWith"
          }
        }
      },
      {
        "id": "policy_premium.written",
        "type": "currency",
        "match": "all",
        "method": {
          "id": "row",
          "position": "right",
          "tiebreaker": "first"
        },
        "anchor": {
          "start": [
            {
              "text": "five-year summary",
              "type": "startsWith"
            },
            {
              "text": "loss ratio",
              "type": "startsWith"
            }
          ],
          "match": {
            "pattern": "^20\\d{2}",
            "type": "regex"
          },
          "end": {
            "text": "totals:",
            "type": "startsWith"
          }
        }
      },
      {
        "id": "policy_number",
        "method": {
          "id": "row",
          "position": "right",
          "tiebreaker": "first"
        },
        "anchor": "policy number:"
      },
      {
        "id": "claim_number",
        "match": "all",
        "method": {
          "id": "passthrough"
        },
        "anchor": {
          "match": {
            "type": "regex",
            "pattern": "^[A-Z0-9]{10,13}$"
          }
        }
      },
      {
        "id": "claim_type",
        "match": "all",
        "method": {
          "id": "label",
          "position": "below"
        },
        "anchor": {
          "match": {
            "type": "equals",
            "text": "company"
          }
        }
      },
      {
        "id": "loss_date",
        "type": "date",
        "match": "all",
        "method": {
          "id": "row",
          "position": "right",
          "tiebreaker": "first"
        },
        "anchor": {
          "match": {
            "type": "regex",
            "pattern": "^[A-Z]$"
          }
        }
      },
      {
        "id": "claim_date",
        "type": "date",
        "match": "all",
        "method": {
          "id": "region",
          "start": "below",
          "width": 1,
          "height": 0.15,
          "offsetX": 0.2,
          "offsetY": 0.02
        },
        "anchor": {
          "match": {
            "type": "regex",
            "pattern": "^[A-Z]$"
          }
        }
      },
      {
