{
  "fingerprint": {
    "tests": [
      {
        "page": "first",
        "match": [
          {
            "text": "Loss Analysis Report",
            "type": "startsWith",
            "isCaseSensitive": true
          },
          {
            "text": "Brokerage",
            "type": "startsWith",
            "isCaseSensitive": true
          }
        ]
      },
      {
        "page": "every",
        "match": [
          {
            "text": "Claims",
            "type": "startsWith",
            "isCaseSensitive": true
          },
          {
            "text": "Claim ID",
            "type": "startsWith",
            "isCaseSensitive": true
          }
        ]
      }
    ]
  },
  "fields": [
    {
      "id": "insured",
      "method": {
        "id": "label",
        "position": "right"
      },
      "anchor": {
        "match": {
          "text": "Name",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "policy_start_date",
      "type": "date",
      "method": {
        "id": "label",
        "position": "right"
      },
      "anchor": {
        "match": {
          "text": "inception date",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "policy_end_date",
      "type": "date",
      "method": {
        "id": "label",
        "position": "right"
      },
      "anchor": {
        "match": {
          "text": "expiration date",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "policy_premium",
      "type": "currency",
      "method": {
        "id": "passthrough"
      },
      "anchor": "not configured not found"
    },
    {
      "id": "policy_number",
      "method": {
        "id": "passthrough"
      },
      "anchor": "not configured, not found"
    },
    {
      "id": "open_claims",
      "type": "number",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first",
        "xRangeFilter": {
          "start": "right",
          "offsetX": 0,
          "width": 1
        }
      },
      "anchor": {
        "start": {
          "text": "totals",
          "type": "equals"
        },
        "match": {
          "text": "open",
          "type": "endsWith"
        },
        "end": "closed"
      }
    },
    {
      "id": "closed_claims",
      "type": "number",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first",
        "xRangeFilter": {
          "start": "right",
          "offsetX": 0,
          "width": 1
        }
      },
      "anchor": {
        "start": {
          "text": "totals",
          "type": "equals"
        },
        "match": {
          "text": "closed",
          "type": "endsWith"
        },
        "end": "total"
      }
    },
    {
      "id": "total_claims",
      "type": "number",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first",
        "xRangeFilter": {
          "start": "right",
          "offsetX": 0,
          "width": 1
        }
      },
      "anchor": {
        "start": {
          "text": "totals",
          "type": "equals"
        },
        "match": {
          "text": "total",
          "type": "endsWith"
        },
        "end": "disability"
      }
    }
  ],
  "sections": [
    {
      "id": "claims_details",
      "range": {
        "anchor": {
          "start": {
            "type": "startsWith",
            "text": "Claims",
            "isCaseSensitive": true
          },
          "match": {
            "type": "regex",
            "pattern": "^\\d{7,9}"
          }
        }
      },
      "fields": [
        {
          "id": "claim_number",
          "method": {
            "id": "regex",
            "pattern": "^\\d{7,9}"
          },
          "anchor": {
            "match": {
              "type": "regex",
              "pattern": "^\\d{7,9}"
            }
          }
        },
        {
          "id": "claim_type",
          "method": {
            "id": "row",
            "position": "left",
            "tiebreaker": "last"
          },
          "anchor": {
            "match": {
