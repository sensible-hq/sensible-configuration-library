{
    "fingerprint": {
      "tests": [
        {
          "page": "first",
          "match": [
            {
              "text": "Printed from Chase for Business",
              "type": "equals",
              "isCaseSensitive": true
            }
          ]
        }
      ]
    },
    "preprocessors": [
      {
        "type": "splitLines",
        "minSpaces": 2
      },
      {
        "type": "removeFooter",
        "offsetY": -0.01,
        "match": {
          "text": "©",
          "type": "startsWith"
        }
      },
      {
        "type": "ocr",
        "matchAll": true,
        "match": {
          "type": "any",
          "matches": [
            {
              "text": "CHASE",
              "type": "equals",
              "isCaseSensitive": true
            },
            {
              "text": "###",
              "type": "endsWith"
            }
          ]
        }
      }
    ],
    "fields": [
      {
        "id": "start_date",
        "type": "date",
        "method": {
          "id": "passthrough"
        },
        "anchor": {
          "match": "not found on this format"
        }
      },
      {
        "id": "end_date",
        "type": "date",
        "method": {
          "id": "passthrough"
        },
        "anchor": {
          "match": "not found on this format"
        }
      },
      {
        "id": "customer_name",
        "method": {
          "id": "passthrough"
        },
        "anchor": {
          "match": "not found on this format"
        }
      },
      {
        "id": "customer_address",
        "type": "address",
        "method": {
          "id": "passthrough"
        },
        "anchor": {
          "match": "not found on this format"
        }
      },
      {
        "id": "account_summary_table",
        "type": "sections",
        "range": {
          "offsetY": -1.2,
          "anchor": {
            "match": {
              "text": "available balance",
              "type": "equals"
            },
            "end": {
              "text": "present balance",
              "type": "startsWith"
            }
          },
          "stop": {
            "text": "present balance",
            "type": "startsWith"
          }
        },
        "fields": [
          {
            "id": "account_name",
            "method": {
              "id": "passthrough"
            },
            "anchor": {
              "match": "not found, not configured"
            }
          },
          {
            "id": "account_number",
            "method": {
              "id": "passthrough"
            },
            "anchor": {
              "match": "not found, not configured"
            }
          },
          {
            "id": "beginning_balance",
            "type": {
              "id": "currency",
              "accountingNegative": "suffixNegativeSign"
            },
            "method": {
              "id": "passthrough"
            },
            "anchor": {
              "match": "not found, not configured"
            }
          },
          {
            "id": "end_balance",
            "type": {
              "id": "currency",
              "accountingNegative": "suffixNegativeSign"
            },
            "anchor": {
              "match": {
                "text": "present balance",
                "type": "startsWith"
              }
            },
            "method": {
              "id": "label",
              "position": "above"
            }
          }
        ]
      },
      {
        "id": "accounts",
        "type": "sections",
        "range": {
          "offsetY": -1.2,
          "anchor": {
            "match": {
              "text": "available balance",
              "type": "equals"
            },
            "end": {
              "text": "present balance",
              "type": "startsWith"
            }
          }
        },
        "fields": [
          {
            "id": "date-section",
            "type": "sections",
            "range": {
              "anchor": {
                "start": {
                  "text": "description",
                  "type": "equals"
                },
                "match": {
                  "type": "regex",
                  "pattern": "\\d{1,2}\\, \\d{4}$|^Pending$"
                }
              }
            },
            "fields": [
              {
                "id": "_date",
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "\\d{1,2}\\, \\d{4}$|^Pending$"
                  }
                },
                "method": {
                  "id": "passthrough"
                }
              },
              {
                "id": "transaction-section",
                "type": "sections",
                "range": {
                  "anchor": {
                    "match": [
                      {
                        "pattern": "^\\-?\\$|^—$",
                        "type": "regex"
                      },
                      {
                        "pattern": "^\\-?\\$|^—$",
                        "type": "regex"
                      }
                    ]
                  },
                  "stop": {
                    "type": "any",
                    "matches": [
                      {
                        "text": "Morgan Chase Bank",
                        "type": "startsWith",
                        "isCaseSensitive": true
                      },
                      {
                        "text": "https",
                        "type": "startsWith",
                        "isCaseSensitive": true
                      },
                      {
                        "text": "chase.com",
                        "type": "endsWith",
                        "isCaseSensitive": true
                      },
                      {
                        "text": "date",
                        "type": "equals"
                      }
                    ]
                  },
                  "stopOffsetY": -0.1
                },
                "fields": [
                  {
                    "id": "date",
                    "method": {
                      "id": "copy_to_section",
                      "source_id": "_date"
                    }
                  },
                  {
                    "id": "description",
                    "match": "first",
                    "anchor": {
                      "match": {
                        "pattern": "^[A-Z].{10,}",
                        "type": "regex",
                        "xRangeFilter": {
                          "minX": 1,
                          "maxX": 5
                        }
                      }
                    },
                    "method": {
                      "id": "label",
                      "position": "below",
                      "stop": "gap",
                      "includeAnchor": true,
                      "sortLines": "readingOrderLeftToRight",
                      "lineFilters": [
                        {
                          "pattern": "^\\-?\\$|^—$",
                          "type": "regex"
                        },
                        {
                          "type": "regex",
                          "pattern": "\\d{1,2}\\, \\d{4}$|^Pending$"
                        },
                        {
                          "text": "atm deposit",
                          "type": "equals"
                        },
                        {
                          "text": "ach debit",
                          "type": "equals"
                        }
                      ]
                    }
                  },
                  {
                    "id": "amount",
                    "type": {
                      "id": "currency",
                      "accountingNegative": "default",
                      "removeSpaces": true
                    },
                    "anchor": {
                      "match": {
                        "pattern": "^\\-?\\$|^—$",
                        "type": "regex",
                        "xRangeFilter": {
                          "minX": 5,
                          "maxX": 6.6
                        }
                      }
                    },
                    "method": {
                      "id": "passthrough"
                    }
                  }
                ]
              }
            ]
          },
          {
            "id": "deposits_table",
            "method": {
              "id": "passthrough"
            },
            "anchor": {
              "match": "not found, not configured"
            }
          },
          {
            "id": "withdrawals_table",
            "method": {
              "id": "passthrough"
            },
            "anchor": {
              "match": "not found, not configured"
            }
          },
          {
            "id": "transaction_table",
            "method": {
              "id": "copy_from_sections",
              "source_sections": "date-section",
              "source_field": "transaction-section"
            }
          },
          {
            "id": "clean",
            "method": {
              "id": "suppressOutput",
              "source_ids": [
                "date-section"
              ]
            }
          }
        ]
      }
    ]
  }