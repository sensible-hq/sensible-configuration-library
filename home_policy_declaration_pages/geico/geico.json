{
    "fingerprint": {
      "tests": [
        {
          "page": "any",
          "match": [
            {
              "text": "geico",
              "type": "includes"
            }
          ]
        }
      ]
    },
    "fields": [
      {
        "id": "policy_number",
        "match": "first",
        "anchor": {
          "match": [
            {
              "type": "startsWith",
              "text": "Policy number"
            }
          ]
        },
        "method": {
          "id": "passthrough",
          "wordFilters": [
            "Policy number: "
          ]
        }
      },
      {
        "id": "policy_premium",
        "type": "currency",
        "anchor": {
          "match": [
            {
              "type": "startsWith",
              "text": "Total premium"
            }
          ]
        },
        "method": {
          "id": "label",
          "position": "right"
        }
      },
      {
        "id": "_fee_names_raw",
        "match": "all",
        "method": {
          "id": "passthrough"
        },
        "anchor": {
          "start": [
            {
              "type": "startsWith",
              "text": "YOU AS NAMED INSURED AND ADDRESS"
            }
          ],
          "end": [
            {
              "type": "startsWith",
              "text": "TOTAL PREMIUM"
            }
          ],
          "match": [
            {
              "type": "includes",
              "text": "fee"
            }
          ]
        }
      },
      {
        "id": "fee_values",
        "match": "all",
        "type": "currency",
        "method": {
          "id": "passthrough"
        },
        "anchor": {
          "start": [
            {
              "type": "startsWith",
              "text": "YOU AS NAMED INSURED AND ADDRESS"
            }
          ],
          "end": [
            {
              "type": "startsWith",
              "text": "TOTAL PREMIUM"
            }
          ],
          "match": [
            {
              "type": "includes",
              "text": "fee"
            }
          ]
        }
      },
      {
        "id": "named_insured",
        "type": "name",
        "match": "all",
        "method": {
          "id": "label",
          "position": "below",
          "lineFilters": [
            {
              "type": "regex",
              "pattern": "[0-9]"
            }
          ],
          "stop": {
            "type": "startsWith",
            "text": "Your billing"
          }
        },
        "anchor": {
          "match": {
            "type": "startsWith",
            "text": "POLICYHOLDER INFORMATION"
          }
        }
      },
      {
        "id": "address",
        "type": "address",
        "method": {
          "id": "label",
          "position": "below",
          "stop": "gap"
        },
        "anchor": {
          "match": [
            {
              "type": "startsWith",
              "text": "POLICYHOLDER INFORMATION"
            }
          ]
        }
      },
      {
        "id": "expiration_date",
        "type": "date",
        "anchor": {
          "match": {
            "type": "startsWith",
            "text": "Coverage dates:"
          }
        },
        "method": {
          "id": "passthrough",
          "tiebreaker": ">"
        }
      },
      {
        "id": "effective_date",
        "type": "date",
        "anchor": {
          "match": {
            "type": "startsWith",
            "text": "Coverage dates:"
          }
        },
        "method": {
          "id": "passthrough",
          "tiebreaker": "<"
        }
      },
      {
        "id": "total",
        "type": "currency",
        "anchor": {
          "match": [
            {
              "type": "startsWith",
              "text": "Total premium"
            }
          ]
        },
        "method": {
          "id": "label",
          "position": "right"
        }
      },
      {
        "id": "dwelling_limit",
        "anchor": {
          "match": [
            {
              "text": "AMOUNT OF COVERAGE",
              "type": "includes"
            },
            {
              "text": "Dwelling",
              "type": "includes"
            }
          ]
        },
        "type": "currency",
        "method": {
          "id": "label",
          "position": "right"
        }
      },
      {
        "id": "deductible",
        "type": "currency",
        "method": {
          "id": "label",
          "position": "right"
        },
        "anchor": {
          "match": [
            {
              "text": "AMOUNT OF COVERAGE",
              "type": "includes"
            },
            {
              "text": "deductible",
              "type": "includes"
            }
          ]
        }
      },
      {
        "id": "medical_payment_to_others_limit",
        "anchor": {
          "match": [
            {
              "text": "AMOUNT OF COVERAGE",
              "type": "includes"
            },
            {
              "text": "medical payments",
              "type": "includes"
            }
          ]
        },
        "type": "currency",
        "method": {
          "id": "label",
          "position": "right"
        }
      },
      {
        "id": "personal_liability_limit",
        "anchor": {
          "match": [
            {
              "text": "AMOUNT OF COVERAGE",
              "type": "includes"
            },
            {
              "text": "personal liability",
              "type": "includes"
            }
          ]
        },
        "type": "currency",
        "method": {
          "id": "label",
          "position": "right"
        }
      },
      {
        "id": "loss_of_use_limit",
        "type": "currency",
        "anchor": {
          "start": {
            "text": "AMOUNT OF COVERAGE",
            "type": "includes"
          },
          "match": [
            {
              "text": "Loss of Use",
              "type": "includes"
            }
          ]
        },
        "method": {
          "id": "label",
          "position": "right"
        }
      },
      {
        "id": "personal_property_limit",
        "anchor": {
          "match": [
            {
              "text": "AMOUNT OF COVERAGE",
              "type": "includes"
            },
            {
              "text": "Personal Property",
              "type": "includes"
            }
          ]
        },
        "type": "currency",
        "method": {
          "id": "label",
          "position": "right"
        }
      },
      {
        "id": "other_structures_limit",
        "anchor": {
          "match": [
            {
              "text": "AMOUNT OF COVERAGE",
              "type": "includes"
            },
            {
              "text": "Other Structures",
              "type": "includes"
            }
          ]
        },
        "type": "currency",
        "method": {
          "id": "label",
          "position": "right"
        }
      },
      {
        "id": "deductible_wind_hail",
        "anchor": {
          "start": [
            {
              "text": "AMOUNT OF COVERAGE",
              "type": "includes"
            }
          ],
          "match": {
            "text": "Hurricane",
            "type": "includes"
          }
        },
        "type": "currency",
        "method": {
          "id": "label",
          "position": "right"
        }
      }
    ],
    "computed_fields": [
      {
        "id": "fee_names",
        "method": {
          "id": "split",
          "source_id": "_fee_names_raw",
          "separator": " $",
          "index": 0
        }
      }
    ]
  }