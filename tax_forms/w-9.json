{
  "fingerprint": {
    "tests": [
      {
        "page": "any",
        "match": [
          {
            "text": "w-9",
            "type": "includes"
          },
          {
            "text": "Identification Number and Certification",
            "type": "includes"
          }
        ]
      }
    ]
  },
    "fields": [
      {
        "id": "name",
        "method": {
          "id": "label",
          "position": "below"
        },
        "anchor": {
          "match": {
            "text": "name (as shown",
            "type": "includes"
          }
        }
      },
      {
        "id": "business_name",
        "method": {
          "id": "label",
          "position": "below"
        },
        "anchor": {
          "match": {
            "text": "business name/",
            "type": "includes"
          }
        }
      },
      {
        "id": "federal_tax_classification.individual_sole_proprietor_single_member_llc",
        "method": {
          "id": "checkbox",
          "position": "left"
        },
        "anchor": {
          "match": {
            "text": "individual/sole proprietor",
            "type": "includes"
          }
        }
      },
      {
        "id": "federal_tax_classification.c_corporation",
        "method": {
          "id": "checkbox",
          "position": "left"
        },
        "anchor": {
          "match": {
            "text": "c corporation",
            "type": "includes"
          }
        }
      },
      {
        "id": "federal_tax_classification.s_corporation",
        "method": {
          "id": "checkbox",
          "position": "left"
        },
        "anchor": {
          "match": {
            "text": "s corporation",
            "type": "includes"
          }
        }
      },
      {
        "id": "federal_tax_classification.partnership",
        "method": {
          "id": "checkbox",
          "position": "left"
        },
        "anchor": {
          "match": {
            "text": "partnership",
            "type": "includes"
          }
        }
      },
      {
        "id": "federal_tax_classification.trust_estate",
        "method": {
          "id": "checkbox",
          "position": "left"
        },
        "anchor": {
          "match": {
            "text": "trust/estate",
            "type": "includes"
          }
        }
      },
      {
        "id": "exempt_payee_code",
        "method": {
          "id": "label",
          "position": "right"
        },
        "anchor": {
          "match": {
            "text": "exempt payee code",
            "type": "startsWith"
          }
        }
      },
      {
        "id": "exemption_from_fatca_reporting_code",
        "method": {
          "id": "label",
          "position": "right"
        },
        "anchor": {
          "start": "exemption from fatca reporting",
          "match": {
            "text": "code (if any)",
            "type": "includes"
          }
        }
      },
      {
        "id": "street_address",
        "method": {
          "id": "label",
          "position": "below"
        },
        "anchor": {
          "match": {
            "text": "address (number",
            "type": "includes"
          }
        }
      },
      {
        "id": "city_state_zip_address",
        "method": {
          "id": "label",
          "position": "below"
        },
        "anchor": {
          "match": {
            "text": "city, state, and zip",
            "type": "includes"
          }
        }
      },
      {
        "id": "requesters_name",
        "method": {
          "id": "region",
          "start": "left",
          "offsetX": -0.05,
          "offsetY": 0,
          "height": 1,
          "width": 3,
          "lineFilters": [
            {
              "type": "regex",
              "pattern": "^\\d"
            },
            {
              "type": "regex",
              "pattern": "\\d$"
            }
          ]
        },
        "anchor": {
          "match": {
            "text": "Requester’s name and address",
            "type": "includes"
          }
        }
      },
      {
        "id": "requesters_address",
        "type": "address",
        "method": {
          "id": "region",
          "start": "left",
          "offsetX": -0.05,
          "offsetY": 0,
          "height": 1,
          "width": 3
        },
        "anchor": {
          "match": {
            "text": "Requester’s name and address",
            "type": "includes"
          }
        }
      },
      {
        "id": "ssn",
        "method": {
          "id": "region",
          "start": "left",
          "offsetX": -0.05,
          "offsetY": 0,
          "height": 0.5,
          "width": 2.4,
          "xMajorSort": true
        },
        "anchor": {
          "match": {
            "text": "social security number",
            "type": "includes"
          }
        }
      },
      {
        "id": "ein",
        "method": {
          "id": "region",
          "start": "left",
          "offsetX": -0.05,
          "offsetY": 0,
          "height": 0.5,
          "width": 2.4,
          "xMajorSort": true
        },
        "anchor": {
          "start": {
            "text": "or",
            "type": "equals"
          },
          "match": {
            "text": "employer identification number",
            "type": "includes"
          }
        }
      }
    ]
  }