          "id": "row",
          "position": "left",
          "tiebreaker": "last"
        },
        "anchor": {
          "match": [
            {
              "text": "AND MEDICAL EXPENSES",
              "type": "endsWith",
              "isCaseSensitive": true
            },
            {
              "pattern": "BP0404|BP0420",
              "type": "regex"
            },
