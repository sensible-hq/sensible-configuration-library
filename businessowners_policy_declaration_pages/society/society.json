{
    "fingerprint": {
      "tests": [
        {
          "page": "any",
          "match": [
            {
              "pattern": "SOCIETY INSURANCE",
              "type": "regex"
            }
          ]
        }
      ]
    },
    "fields": [
      {
        "id": "account_company_name",
        "method": {
          "id": "region",
          "start": "below",
          "width": 3,
          "height": 0.9,
          "offsetX": -1.5,
          "offsetY": 0.01,
          "typeFilters": [
            "address"
          ]
        },
        "anchor": {
          "match": {
            "text": "named insured",
            "type": "startsWith"
