        "type": "currency",
        "method": {
          "id": "row",
          "position": "right",
          "tiebreaker": "first"
        },
        "anchor": {
          "start": {
            "text": "AND MEDICAL EXPENSES",
            "type": "endsWith",
            "isCaseSensitive": true
          },
          "match": [
            {
              "text": "medical expenses",
              "type": "equals"
            }
