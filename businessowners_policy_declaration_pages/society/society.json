        }
      },
      {
        "id": "deductible",
        "type": "currency",
        "method": {
          "id": "box",
          "position": "below",
          "offsetY": 0.26,
          "tiebreaker": "first"
        },
        "anchor": {
          "start": {
            "text": "described premises",
            "type": "startsWith"
          },
          "match": [
            {
              "type": "equals",
              "text": "property deductible"
            }
          ],
          "end": {
            "type": "any",
            "matches": [
              {
                "text": "total premium",
                "type": "equals"
              }
            ]
          }
        }
      },
      {
        "id": "deductible",
        "type": "currency",
        "method": {
          "id": "box",
          "position": "right",
          "offsetX": 1,
          "tiebreaker": "first"
        },
        "anchor": {
          "start": {
            "text": "described premises",
            "type": "startsWith"
          },
          "match": [
            {
              "type": "equals",
              "text": "property deductible"
            },
            {
              "text": "location 1",
              "type": "includes"
            }
          ],
          "end": {
            "type": "any",
            "matches": [
              {
                "text": "total premium",
                "type": "equals"
              }
            ]
          }
        }
      },
      {
        "id": "effective_date",
        "type": "date",
        "method": {
          "id": "label",
          "position": "right",
          "tiebreaker": "<"
