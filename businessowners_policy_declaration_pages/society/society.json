        "method": {
          "id": "row",
          "position": "right",
          "tiebreaker": "last",
          "whitespaceFilter": "all"
        },
        "anchor": {
          "start": {
            "text": "described premises",
            "type": "equals"
          },
          "match": {
            "text": "buildings",
            "type": "equals"
          },
          "end": {
            "type": "any",
            "matches": [
              {
                "text": "total premium",
                "type": "equals"
              }
            ]
