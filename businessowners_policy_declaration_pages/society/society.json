        "anchor": {
          "match": {
            "text": "policy period:",
            "type": "startsWith"
          }
        }
      },
      {
        "id": "expiration_date",
        "type": "date",
        "method": {
          "id": "label",
          "position": "right",
          "tiebreaker": ">"
        },
        "anchor": {
          "match": {
            "text": "policy period:",
            "type": "startsWith"
          }
        }
      },
      {
        "id": "annual_gross_premium",
        "type": "currency",
        "method": {
          "id": "row",
          "position": "right",
          "tiebreaker": "first"
        },
        "anchor": {
          "match": {
            "text": "total premium",
            "type": "equals"
          }
        }
      },
      {
        "id": "each_occurence.amount",
        "type": "currency",
        "method": {
          "id": "row",
          "position": "right",
          "tiebreaker": "first"
        },
        "anchor": {
          "start": {
            "text": "AND MEDICAL EXPENSES",
            "type": "endsWith",
            "isCaseSensitive": true
