          "start": [
            {
              "text": "described premises",
              "type": "equals"
            },
            {
              "text": "type of property",
              "type": "equals"
            }
          ],
          "match": {
            "text": "buildings",
            "type": "equals"
          },
          "end": {
            "type": "any",
            "matches": [
              {
                "text": "total premium",
                "type": "equals"
              }
            ]
          }
        }
      },
      {
        "id": "business_personal_property.amount",
        "match": "all",
        "method": {
          "id": "row",
          "position": "right",
          "tiebreaker": "last",
          "whitespaceFilter": "all"
