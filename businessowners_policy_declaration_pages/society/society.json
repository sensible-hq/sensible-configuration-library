            "type": "equals",
            "isCaseSensitive": true
          }
        }
      },
      {
        "id": "policy_number",
        "method": {
          "id": "label",
          "position": "right"
        },
        "anchor": {
          "match": {
            "type": "any",
            "matches": [
              {
                "text": "policy no:",
                "type": "startsWith"
