          "height": 0.22,
          "offsetX": -1.9,
          "offsetY": -0.1
        },
        "anchor": {
          "start": [
            {
              "text": "described premises",
              "type": "equals"
            },
            {
              "text": "type of property",
              "type": "equals"
            }
          ],
          "match": [
            {
              "text": "business personal",
              "type": "startsWith"
