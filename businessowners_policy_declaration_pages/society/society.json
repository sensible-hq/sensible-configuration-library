            "type": "startsWith"
          }
        }
      },
      {
        "id": "aggregate.amount",
        "type": "currency",
        "method": {
          "id": "row",
          "position": "right",
          "tiebreaker": "first"
        },
        "anchor": {
          "start": {
            "text": "AND MEDICAL EXPENSES",
            "type": "endsWith",
            "isCaseSensitive": true
          },
          "match": {
            "text": "other than products",
            "type": "startsWith"
          }
        }
      },
      {
        "id": "damage_to_rented_premises.amount",
        "type": "currency",
        "method": {
          "id": "row",
          "position": "right",
          "tiebreaker": "last"
        },
        "anchor": {
          "start": {
            "text": "AND MEDICAL EXPENSES",
            "type": "endsWith",
            "isCaseSensitive": true
          },
          "match": [
            {
              "text": "damage to premises",
              "type": "startsWith"
            }
