{
  "fingerprint": {
    "tests": [
      {
        "page": "any",
        "match": [
          {
            "text": "branch",
            "type": "includes"
          }
        ]
      }
    ]
  },
  "fields": [
    {
      "id": "policy_number",
      "method": {
        "id": "label",
        "position": "below"
      },
      "anchor": {
        "match": [
          {
            "text": "Policy Number:",
            "type": "startsWith"
          }
        ]
      }
    },
    {
      "id": "policy_premium",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "<"
      },
      "anchor": {
        "match": [
          {
            "text": "premium for automobile(s) insured",
            "type": "equals"
          }
        ]
      }
    },
    {
      "id": "fees",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "<"
      },
      "anchor": {
        "match": [
          {
            "text": "fees",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "total",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "<"
      },
      "anchor": {
        "match": [
          {
            "text": "total price",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "effective_date",
      "type": "date",
      "method": {
        "id": "label",
        "position": "below",
        "tiebreaker": "<"
      },
      "anchor": {
        "match": [
          {
            "text": "policy period:",
            "type": "equals"
          }
        ]
      }
    },
    {
      "id": "expiration_date",
      "type": "date",
      "method": {
        "id": "label",
        "position": "below",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": [
          {
            "text": "policy period:",
            "type": "equals"
          }
        ]
      }
    },
    {
      "id": "address",
      "type": "address",
      "method": {
        "id": "column"
      },
      "anchor": {
        "match": [
          {
            "text": "mailing address",
            "type": "startsWith"
          }
        ],
        "end": "email address:"
      }
    },
    {
      "id": "named_insured",
      "type": "name",
      "match": "all",
      "method": {
        "id": "documentRange",
        "stop": {
          "text": "mailing address",
          "type": "startsWith"
        },
        "xRangeFilter": {
          "start": "left",
          "width": 3,
          "offsetX": 0
        }
      },
      "anchor": {
        "match": [
          {
            "text": "Named Insured",
            "type": "startsWith"
          }
        ]
      }
    },
    {
      "id": "vehicle_year",
      "match": "all",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "type": "startsWith",
          "text": "model year:"
        }
      }
    },
    {
      "id": "vehicle_make",
      "match": "all",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "last"
      },
      "anchor": {
        "match": [
          {
            "type": "includes",
            "text": "model year:"
          }
        ]
      }
    },
    {
      "id": "vehicle_model",
      "match": "all",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "last"
      },
      "anchor": {
        "match": [
          {
            "type": "includes",
            "text": "model year:"
          }
        ]
      }
    },
    {
      "id": "vin",
      "match": "all",
      "method": {
        "id": "regex",
        "pattern": "([A-Z0-9]{12}\\d{5})"
      },
      "anchor": {
        "start": [
          {
            "text": "Vehicle",
            "type": "startsWith"
          }
        ],
        "match": [
          {
            "pattern": "[A-Z0-9]{12}\\d{5}$",
            "type": "regex"
          }
        ]
      }
    },
    {
      "id": "bodily_injury_liability_per_person",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": [
