        "start": [
          {
            "pattern": "(UM/UIM|UNINSURED)",
            "type": "regex"
          }
        ],
        "match": [
          {
            "text": "bodily",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "uninsured_underinsured_motorist_bodily_injury_per_accident",
      "type": "currency",
      "method": {
        "id": "label",
        "position": "right"
      },
      "anchor": {
        "start": [
          {
            "pattern": "(UM/UIM|UNINSURED)",
            "type": "regex"
          },
          {
            "text": "bodily",
            "type": "includes"
          }
        ],
        "match": [
          {
            "text": "ea acc",
            "type": "includes"
          }
