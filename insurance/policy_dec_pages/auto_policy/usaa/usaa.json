{
  "preprocessors": [
    {
      "type": "mergeLines",
      "directlyAdjacentThreshold": 0.15,
      "adjacentThreshold": 1
    }
  ],
  "fingerprint": {
    "tests": [
      {
        "page": "any",
        "match": [
          {
            "text": "usaa",
            "type": "includes"
          }
        ]
      }
    ]
  },
  "fields": [
    {
      "id": "policy_number",
      "method": {
        "id": "box",
        "position": "below"
      },
      "anchor": {
        "match": [
          {
            "text": "Policy Number",
            "type": "startsWith"
          }
        ]
      }
    },
    {
      "id": "policy_premium",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": [
          {
            "text": "vehicle total premium",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "policy_premium",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": [
          {
            "pattern": "\\d{1,2} month premium",
            "type": "regex",
            "flags": "i"
          }
        ]
      }
    },
    {
      "id": "total",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": [
          {
            "pattern": "\\d{1,2} month premium",
            "type": "regex",
            "flags": "i"
          }
        ]
      }
    },
    {
      "id": "total",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": [
          {
            "text": "vehicle total premium",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "premium_period",
      "method": {
        "id": "regex",
        "pattern": "(\\d{1,2})-month",
        "flags": "i"
      },
      "anchor": {
        "match": [
          {
            "pattern": "\\d{1,2}-month",
            "type": "regex",
            "flags": "i"
          }
        ]
      }
    },
    {
      "id": "effective_date",
      "type": "date",
      "method": {
        "id": "label",
        "position": "right",
        "tiebreaker": "<"
      },
      "anchor": {
        "match": [
          {
            "text": "Policy Period",
            "type": "startsWith"
          },
          {
            "text": "standard time",
            "type": "includes"
          },
          {
            "text": "effective",
            "type": "startsWith"
          }
        ]
      }
    },
    {
      "id": "expiration_date",
      "type": "date",
      "method": {
        "id": "label",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": [
          {
            "text": "Policy Period",
            "type": "startsWith"
          },
          {
            "text": "standard time",
            "type": "includes"
          },
          {
            "text": "effective",
            "type": "startsWith"
          }
        ]
      }
    },
    {
      "id": "address",
      "type": "address",
      "method": {
        "id": "box",
        "position": "below"
      },
      "anchor": {
        "match": [
          {
            "text": "Named insured and address",
            "type": "startsWith"
          }
        ]
      }
    },
    {
      "id": "named_insured",
      "match": "all",
      "type": "name",
      "method": {
        "id": "regex",
        "pattern": "^\\d{2} ([a-z -]+)",
        "flags": "i"
      },
      "anchor": {
        "start": [
          {
            "text": "Operators",
            "type": "equals"
          }
        ],
        "match": [
          {
            "pattern": "^\\d{2} (\\w+)",
            "type": "regex"
          }
        ],
        "end": [
          {
            "text": "description of",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "vehicle_year",
      "match": "all",
      "method": {
        "id": "regex",
        "pattern": "\\d{2} (\\d{2}) \\w+"
      },
      "anchor": {
        "start": [
          {
            "text": "Identification Number",
            "type": "startsWith"
          }
        ],
        "match": [
          {
            "pattern": "\\d{2} \\d{2} \\w+",
            "type": "regex"
          }
        ],
        "end": [
          {
            "text": "The Vehicle(s) described",
            "type": "startsWith"
          }
        ]
      }
    },
    {
      "id": "vehicle_make",
      "match": "all",
      "method": {
        "id": "regex",
        "pattern": "\\d{2} \\d{2} (\\w+)"
      },
      "anchor": {
        "start": [
          {
            "text": "Identification Number",
            "type": "startsWith"
          }
        ],
        "match": [
          {
            "pattern": "\\d{2} \\d{2}",
            "type": "regex"
          }
        ],
        "end": [
          {
            "text": "The Vehicle(s) described",
            "type": "startsWith"
          }
        ]
      }
    },
    {
      "id": "vehicle_model",
      "match": "all",
      "method": {
        "id": "row",
        "position": "left",
        "tiebreaker": "second"
      },
      "anchor": {
        "start": [
          {
            "text": "Description of Vehicle",
            "type": "startsWith"
          }
        ],
        "match": [
          {
            "pattern": "^.{12}\\d{5}$",
            "type": "regex"
          }
        ]
      }
    },
    {
      "id": "vin",
      "match": "all",
      "method": {
        "id": "passthrough"
      },
      "anchor": {
        "start": [
          {
            "text": "Description",
            "type": "startsWith"
          }
        ],
        "match": [
          {
            "pattern": "^.{12}\\d{5}$",
            "type": "regex"
          }
        ]
      }
    },
    {
      "id": "bodily_injury_liability_per_person",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": [
          {
            "text": "bodily injury",
            "type": "startsWith"
          }
        ]
      }
    },
    {
      "id": "bodily_injury_liability_per_accident",
      "type": "currency",
      "method": {
        "id": "label",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": [
          {
            "text": "bodily",
            "type": "startsWith"
          }
        ],
        "match": [
          {
            "text": "ea acc",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "bodily_injury_liability_per_accident",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": [
          {
            "text": "bodily",
            "type": "startsWith"
          }
        ],
        "match": [
          {
            "text": "ea acc",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "property_damage_liability_per_accident",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": [
          {
            "text": "property damage",
            "type": "startsWith"
          }
        ],
        "match": [
          {
            "text": "ea acc",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "medical_payment_to_others_limit",
      "type": "currency",
      "method": {
        "id": "label",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": [
          {
            "text": "medical payments",
            "type": "includes"
          }
        ],
        "match": [
          {
            "text": "ea per",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "medical_payment_to_others_limit",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": [
          {
            "text": "medical payments",
            "type": "includes"
          }
        ],
        "match": [
          {
            "text": "ea per",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "personal_injury_protection_per_person",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": [
          {
            "text": "b2-personal",
            "type": "includes"
          }
        ],
        "match": [
          {
            "type": "includes",
            "text": "ea person"
          }
        ]
      }
    },
    {
      "id": "uninsured_underinsured_property_damage_deductible",
      "type": "currency",
      "method": {
        "id": "regex",
        "pattern": "D[ ]?(\\d+)"
      },
      "anchor": {
        "start": [
          {
            "pattern": "(UM/UIM|UNINSURED)",
            "type": "regex"
          },
          {
            "text": "property",
            "type": "startsWith"
          }
        ],
        "match": [
          {
            "text": "d",
            "type": "includes"
          }
        ],
        "end": [
          {
            "text": "Part D",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "uninsured_underinsured_property_damage_per_accident",
      "type": "currency",
