    {
      "id": "_collision_limit",
      "method": {
        "id": "row",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": [
          {
            "text": "collision",
            "type": "startsWith"
          }
        ]
      }
    },
    {
      "id": "collision_deductible",
      "type": "currency",
      "method": {
        "id": "regex",
        "pattern": "D[ ]?(\\d+)"
      },
      "anchor": {
        "match": [
          {
            "text": "collision loss",
            "type": "startsWith"
          },
          {
            "text": "D",
            "type": "startsWith"
          }
        ],
        "end": [
          {
            "text": "total premium",
            "type": "includes"
          }
        ]
      }
    },
    {
      "id": "_comprehensive_limit",
      "method": {
