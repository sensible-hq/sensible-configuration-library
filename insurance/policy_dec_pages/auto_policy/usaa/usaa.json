  ],
  "computed_fields": [
    {
      "id": "collision_limit",
      "method": {
        "id": "mapper",
        "source_id": "_collision_limit",
        "mappings": {
          "ACV LESS": "Actual Cash Value"
        }
      }
    },
    {
      "id": "comprehensive_limit",
      "method": {
        "id": "mapper",
        "source_id": "_comprehensive_limit",
