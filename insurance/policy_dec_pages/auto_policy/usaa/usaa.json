{
  "preprocessors": [
    {
      "type": "mergeLines",
      "directlyAdjacentThreshold": 0.15,
      "adjacentThreshold": 1
    }
  ],
  "fields": [
    {
      "id": "policy_number",
      "method": {
        "id": "box",
        "position": "below"
      },
      "anchor": {
        "match": {
          "text": "policy number",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "effective_date",
      "type": "date",
      "method": {
        "id": "label",
        "position": "right",
        "tiebreaker": "<"
      },
      "anchor": {
        "start": [
          {
            "text": "policy period",
            "type": "startsWith"
          },
          {
            "text": "standard time",
            "type": "includes"
          }
        ],
        "match": {
          "text": "effective",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "expiration_date",
      "type": "date",
      "method": {
        "id": "label",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": [
          {
            "text": "policy period",
            "type": "startsWith"
          },
          {
            "text": "standard time",
            "type": "includes"
          }
        ],
        "match": {
          "text": "effective",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "named_insured",
      "match": "all",
      "type": "name",
      "method": {
        "id": "regex",
        "pattern": "^\\d{2} ([a-z -]+)",
        "flags": "i"
      },
      "anchor": {
        "start": {
          "text": "operators",
          "type": "equals"
        },
        "match": {
          "pattern": "^\\d{2} (\\w+)",
          "type": "regex"
        },
        "end": {
          "text": "description of",
          "type": "includes"
        }
      }
    },
    {
      "id": "address",
      "type": "address",
      "method": {
        "id": "box",
        "position": "below"
      },
      "anchor": {
        "match": {
          "text": "named insured and address",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "vehicle_year",
      "match": "all",
      "method": {
        "id": "regex",
        "pattern": "\\d{2} (\\d{2}) \\w+"
      },
      "anchor": {
        "start": {
          "text": "identification Number",
          "type": "startsWith"
        },
        "match": {
          "pattern": "\\d{2} \\d{2} \\w+",
          "type": "regex"
        },
        "end": {
          "text": "The vehicle(s) described",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "vehicle_make",
      "match": "all",
      "method": {
        "id": "regex",
        "pattern": "\\d{2} \\d{2} (\\w+)"
      },
      "anchor": {
        "start": {
          "text": "identification number",
          "type": "startsWith"
        },
        "match": {
          "pattern": "\\d{2} \\d{2}",
          "type": "regex"
        },
        "end": {
          "text": "the vehicle(s) described",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "vehicle_model",
      "match": "all",
      "method": {
        "id": "row",
        "position": "left",
        "tiebreaker": "second"
      },
      "anchor": {
        "start": {
          "text": "description of vehicle",
          "type": "startsWith"
        },
        "match": {
          "pattern": "^.{12}\\d{5}$",
          "type": "regex"
        }
      }
    },
    {
      "id": "vin",
      "match": "all",
      "method": {
        "id": "passthrough"
      },
      "anchor": {
        "start": {
          "text": "description",
          "type": "startsWith"
        },
        "match": {
          "pattern": "^.{12}\\d{5}$",
          "type": "regex"
        }
      }
    },
    {
      "id": "premium_period",
      "method": {
        "id": "regex",
        "pattern": "(\\d{1,2})-month",
        "flags": "i"
      },
      "anchor": {
        "match": {
          "pattern": "\\d{1,2}-month",
          "type": "regex",
          "flags": "i"
        }
      }
    },
    {
      "id": "bodily_injury_liability_per_person",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": {
          "text": "bodily injury",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "bodily_injury_liability_per_accident",
      "type": "currency",
      "method": {
        "id": "label",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": {
          "text": "bodily",
          "type": "startsWith"
        },
        "match": {
          "text": "ea acc",
          "type": "includes"
        }
      }
    },
    {
      "id": "bodily_injury_liability_per_accident",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": {
          "text": "bodily",
          "type": "startsWith"
        },
        "match": {
          "text": "ea acc",
          "type": "includes"
        }
      }
    },
    {
      "id": "property_damage_liability_per_accident",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": {
          "text": "property damage",
          "type": "startsWith"
        },
        "match": {
          "text": "ea acc",
          "type": "includes"
        }
      }
    },
    {
      "id": "personal_injury_protection_per_person",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": {
          "text": "b2-personal",
          "type": "includes"
        },
        "match": {
          "type": "includes",
          "text": "ea person"
        }
      }
    },
    {
      "id": "uninsured_underinsured_motorist_bodily_injury_per_person",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": {
          "pattern": "(UM/UIM|UNINSURED)",
          "type": "regex"
        },
        "match": {
          "text": "bodily",
          "type": "includes"
        }
      }
    },
    {
      "id": "uninsured_underinsured_motorist_bodily_injury_per_accident",
      "type": "currency",
      "method": {
        "id": "label",
        "position": "right"
      },
      "anchor": {
        "start": [
          {
            "pattern": "(UM/UIM|UNINSURED)",
            "type": "regex"
          },
          {
            "text": "bodily",
            "type": "includes"
          }
        ],
        "match": {
          "text": "ea acc",
          "type": "includes"
        }
      }
    },
    {
      "id": "uninsured_underinsured_motorist_bodily_injury_per_accident",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": [
          {
            "pattern": "(UM/UIM|UNINSURED)",
            "type": "regex"
          },
          {
            "text": "bodily",
            "type": "includes"
          }
        ],
        "match": {
          "text": "ea acc",
          "type": "includes"
        }
      }
    },
    {
      "id": "uninsured_underinsured_property_damage_deductible",
      "type": "currency",
      "method": {
        "id": "regex",
        "pattern": "D[ ]?(\\d+)"
      },
      "anchor": {
        "start": [
          {
            "pattern": "(UM/UIM|UNINSURED)",
            "type": "regex"
          },
          {
            "text": "property",
            "type": "startsWith"
          }
        ],
        "match": {
          "text": "d",
          "type": "includes"
        },
        "end": {
          "text": "Part D",
          "type": "includes"
        }
      }
    },
    {
      "id": "uninsured_underinsured_property_damage_per_accident",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "start": {
          "pattern": "(UM/UIM|UNINSURED)",
          "type": "regex"
        },
        "match": {
          "text": "property",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "comprehensive_limit",
      "method": {
        "id": "row",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "text": "comprehensive",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "comprehensive_deductible",
      "type": "currency",
      "method": {
        "id": "regex",
        "pattern": "D[ ]?(\\d+)"
      },
      "anchor": {
        "start": {
          "text": "comprehensive loss",
          "type": "startsWith"
        },
        "match": {
          "text": "d",
          "type": "startsWith"
        },
        "end": {
          "text": "collision",
          "type": "includes"
        }
      }
    },
    {
      "id": "collision_deductible",
      "type": "currency",
      "method": {
        "id": "regex",
        "pattern": "D[ ]?(\\d+)"
      },
      "anchor": {
        "start": {
          "text": "collision loss",
          "type": "startsWith"
        },
        "match": {
          "text": "d",
          "type": "startsWith"
        },
        "end": {
          "text": "total premium",
          "type": "includes"
        }
      }
    },
    {
      "id": "collision_limit",
      "method": {
        "id": "row",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "text": "collision",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "policy_premium",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": {
          "text": "vehicle total premium",
          "type": "includes"
        }
      }
    },
    {
      "id": "policy_premium",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": {
          "pattern": "\\d{1,2} month premium",
          "type": "regex",
          "flags": "i"
        }
      }
    },
    {
      "id": "total",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": {
          "pattern": "\\d{1,2} month premium",
          "type": "regex",
          "flags": "i"
        }
      }
    },
    {
      "id": "total",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": {
          "text": "vehicle total premium",
          "type": "includes"
        }
      }
    }
  ]
}