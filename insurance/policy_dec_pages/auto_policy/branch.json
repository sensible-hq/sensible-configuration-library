{
  "preprocessors": [
    {
      "type": "splitLines",
      "minSpaces": 3
    }
  ],
  "fields": [
    {
      "id": "named_insured",
      "type": "name",
      "match": "all",
      "method": {
        "id": "documentRange",
        "stop": {
          "text": "mailing address",
          "type": "startsWith"
        },
        "xRangeFilter": {
          "start": "left",
          "width": 3,
          "offsetX": 0
        }
      },
      "anchor": {
        "match": {
          "text": "named insured",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "policy_number",
      "method": {
        "id": "label",
        "position": "below"
      },
      "anchor": {
        "match": {
          "text": "policy number:",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "address",
      "type": "address",
      "method": {
        "id": "documentRange",
        "stop": {
          "text": "email",
          "type": "startsWith"
        },
        "xRangeFilter": {
          "start": "left",
          "width": 3,
          "offsetX": -0.1
        }
      },
      "anchor": {
        "match": {
          "text": "mailing address",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "effective_date",
      "type": "date",
      "method": {
        "id": "label",
        "position": "below",
        "tiebreaker": "<"
      },
      "anchor": {
        "match": {
          "text": "policy period:",
          "type": "equals"
        }
      }
    },
    {
      "id": "expiration_date",
      "type": "date",
      "method": {
        "id": "label",
        "position": "below",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": {
          "text": "policy period:",
          "type": "equals"
        }
      }
    },
    {
      "id": "bodily_injury_liability_per_person",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "text": "bodily injury",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "bodily_injury_liability_per_accident",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "second"
      },
      "anchor": {
        "match": {
          "text": "bodily injury",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "property_damage_liability_per_accident",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "text": "property damage",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "uninsured_underinsured_motorist_bodily_injury_per_person",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "text": "underinsured motorist bodily injury",
          "type": "includes"
        }
      }
    },
    {
      "id": "uninsured_underinsured_motorist_bodily_injury_per_accident",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "second"
      },
      "anchor": {
        "match": {
          "text": "underinsured motorist bodily injury",
          "type": "includes"
        }
      }
    },
    {
      "id": "vin",
      "match": "all",
      "method": {
        "id": "passthrough"
      },
      "anchor": {
        "start": {
          "text": "vehicle #",
          "type": "startsWith"
        },
        "match": {
          "pattern": "[A-Z0-9]{12}\\d{5}$",
          "type": "regex"
        }
      }
    },
    {
      "id": "vehicle_year",
      "match": "all",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "type": "startsWith",
          "text": "model year:"
        }
      }
    },
    {
      "id": "_vehicle_make_raw",
      "match": "all",
      "method": {
        "id": "region",
        "start": "right",
        "width": 2.1,
        "height": 0.25,
        "offsetX": 5.1,
        "offsetY": -0.1,
        "wordFilters": [
          "description"
        ]
      },
      "anchor": {
        "match": {
          "type": "startsWith",
          "text": "vin:"
        }
      }
    },
    {
      "id": "comprehensive_limit",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "text": "comprehensive coverage",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "comprehensive_deductible",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "second"
      },
      "anchor": {
        "start": {
          "text": "coverages",
          "type": "startsWith"
        },
        "match": {
          "text": "comprehensive",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "collision_limit",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "text": "collision coverage",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "collision_deductible",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      },
      "anchor": {
        "start": {
          "text": "coverages",
          "type": "startsWith"
        },
        "match": {
          "text": "collision",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "roadside_assistance",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      },
      "anchor": {
        "start": {
          "text": "coverages for vehicle",
          "type": "startsWith"
        },
        "match": {
          "text": "roadside assistance coverage",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "policy_premium",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "<"
      },
      "anchor": {
        "match": {
          "text": "premium for automobile(s) insured",
          "type": "equals"
        }
      }
    },
    {
      "id": "fees",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "<"
      },
      "anchor": {
        "match": {
          "text": "fees",
          "type": "includes"
        }
      }
    },
    {
      "id": "total",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "<"
      },
      "anchor": {
        "match": {
          "text": "total price",
          "type": "includes"
        }
      }
    },
    {
      "id": "uninsured_underinsured_property_damage_per_accident",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "second"
      },
      "anchor": {
        "match": {
          "type": "includes",
          "text": "uninsured motorist property damage"
        }
      }
    },
    {
      "id": "uninsured_underinsured_property_damage_deductible",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "second"
      },
      "anchor": {
        "match": {
          "type": "includes",
          "text": "uninsured motorist property damage"
        }
      }
    }
  ],
  "computed_fields": [
    {
      "id": "_vehicle_make_model_split",
      "method": {
        "id": "split",
        "source_id": "_vehicle_make_raw",
        "separator": ": ",
        "index": 1
      }
    },
    {
      "id": "vehicle_make",
      "method": {
        "id": "split",
        "source_id": "_vehicle_make_model_split",
        "separator": " ",
        "index": 0
      }
    },
    {
      "id": "vehicle_model",
      "method": {
        "id": "split",
        "source_id": "_vehicle_make_model_split",
        "separator": " ",
        "index": 1
      }
    }
  ]
}