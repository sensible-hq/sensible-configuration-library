{
  "fingerprint": {
    "tests": [
      {
        "page": "any",
        "match": [
          {
            "text": "liberty mutual",
            "type": "includes"
          }
        ]
      }
    ]
  },
  "fields": [
    {
      "id": "_premium_period_raw",
      "method": {
        "id": "regex",
        "pattern": "Total (.*) Premium:"
      },
      "anchor": {
        "match": {
          "type": "equals",
          "text": "policy declarations"
        }
      }
    },
    {
      "id": "named_insured",
      "type": "name",
      "match": "all",
      "method": {
        "id": "regex",
        "pattern": "\\d\\. (.*)$",
        "flags": "i"
      },
      "anchor": {
        "start": {
          "text": "Named Insured:",
          "type": "startsWith"
        },
        "match": {
          "type": "regex",
          "pattern": "\\d\\. "
        },
        "end": {
          "text": "policy number:",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "address",
      "type": "address",
      "anchor": {
        "match": {
          "type": "equals",
          "text": "mailing address:"
        }
      },
      "method": {
        "id": "label",
        "position": "below",
        "stop": "gap"
      }
    },
    {
      "id": "policy_number",
      "anchor": {
        "start": {
          "type": "equals",
          "text": "insurance information"
        },
        "match": {
          "type": "equals",
          "text": "policy number:"
        }
      },
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      }
    },
    {
      "id": "effective_date",
      "type": "date",
      "anchor": {
        "match": {
          "type": "equals",
          "text": "policy period:"
        }
