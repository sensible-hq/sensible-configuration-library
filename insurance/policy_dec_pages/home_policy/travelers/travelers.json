    {
      "id": "named_insured",
      "type": "name",
      "match": "all",
      "anchor": {
        "match": {
          "type": "equals",
          "text": "named insured and mailing address",
          "minimumHeight": 0.14
        }
      },
      "method": {
        "id": "label",
        "position": "below",
        "stop": {
          "type": "regex",
          "pattern": "^\\d+",
          "flags": ""
        }
      }
    },
    {
      "id": "named_insured",
      "type": "name",
      "match": "all",
      "anchor": {
        "match": {
          "type": "includes",
          "text": "Insured:"
        }
      },
      "method": {
        "id": "documentRange",
        "stop": {
          "type": "equals",
          "text": "policy"
        }
      }
    },
    {
