{
  "fingerprint": {
    "tests": [
      {
        "page": "any",
        "match": [
          {
            "text": "state farm",
            "type": "includes"
          }
        ]
      }
    ]
  },
  "fields": [
    {
      "id": "policy_number",
      "anchor": {
        "match": {
          "type": "equals",
          "text": "policy number"
        }
      },
      "method": {
        "id": "row",
        "position": "left",
        "tiebreaker": "first"
      }
    },
    {
      "id": "named_insured",
      "type": "name",
      "match": "all",
      "anchor": {
        "match": {
          "type": "startsWith",
          "text": "named insured"
        }
      },
      "method": {
        "id": "label",
        "position": "below"
      }
    },
    {
      "id": "address",
      "type": "address",
      "anchor": {
        "match": {
          "type": "equals",
          "text": "named insured and mailing address"
        }
      },
      "method": {
        "id": "documentRange",
        "stop": {
          "text": "the policy period begins",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "effective_date",
      "type": "date",
      "method": {
        "id": "row",
        "position": "left",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "text": "effective date",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "expiration_date",
      "type": "date",
      "method": {
        "id": "row",
        "position": "left",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "text": "expiration of policy",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "premium_period",
      "type": "number",
      "method": {
        "id": "label",
        "position": "below"
      },
      "anchor": {
        "match": {
          "text": "effective date",
          "type": "equals"
        }
      }
    },
    {
      "id": "personal_property_limit",
      "anchor": {
        "match": {
          "text": "personal property",
          "type": "startsWith"
        }
      },
      "type": "currency",
      "method": {
        "id": "row",
        "position": "left",
        "tiebreaker": ">"
      }
    },
    {
      "id": "loss_of_use_limit",
      "anchor": "loss of use",
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      }
    },
    {
      "id": "personal_liability_limit",
      "type": "currency",
      "anchor": {
        "match": {
          "text": "personal liability",
          "type": "startsWith"
        }
      },
      "method": {
        "id": "row",
        "position": "left",
        "tiebreaker": ">"
      }
    },
    {
      "id": "medical_payment_to_others_limit",
      "type": "currency",
      "anchor": {
        "match": {
          "text": "medical payments",
          "type": "startsWith"
        }
      },
      "method": {
        "id": "row",
        "position": "left",
        "tiebreaker": ">"
      }
    },
    {
      "id": "deductible",
      "anchor": {
        "match": {
          "text": "deductibles",
          "type": "startsWith"
        }
      },
      "type": "currency",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": ">"
      }
    },
    {
      "id": "policy_premium",
      "type": "currency",
      "anchor": "policy premium",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      }
    }
  ]
}