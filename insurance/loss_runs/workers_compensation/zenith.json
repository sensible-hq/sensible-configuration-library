        {
          "id": "body_parts",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "second"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "part of body"
            }
          }
        },
        {
          "id": "location",
          "method": {
            "id": "passthrough"
          },
          "anchor": "not configured not found"
        },
        {
          "id": "litigation",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "first"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "litigation flag"
            }
          }
        }
      ]
    }
  ]
}