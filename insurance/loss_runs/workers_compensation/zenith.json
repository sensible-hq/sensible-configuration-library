          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "first"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "medical"
            }
          }
        },
        {
          "id": "claim_amount_paid.indemnity",
          "type": "currency",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "first"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "indemnity"
            }
          }
        },
        {
          "id": "claim_amount_paid.expense",
          "type": "currency",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "first"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "expense"
            }
          }
        },
        {
          "id": "claim_amount_incurred.total",
          "type": "currency",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "third"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "total"
            }
          }
        },
        {
          "id": "claim_amount_incurred.medical",
          "type": "currency",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "third"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "medical"
            }
          }
        },
        {
          "id": "claim_amount_incurred.indemnity",
          "type": "currency",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "third"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "indemnity"
            }
          }
        },
        {
          "id": "claim_amount_incurred.expense",
          "type": "currency",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "third"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "expense"
            }
          }
        },
        {
          "id": "accident_description",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "second"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "accident"
            }
          }
        },
        {
          "id": "nature_of_injuries",
          "method": {
            "id": "row",
            "position": "right",
