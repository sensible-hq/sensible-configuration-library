          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "first"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "total"
            }
          }
        },
        {
          "id": "claim_amount_paid.medical",
