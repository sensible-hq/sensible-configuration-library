{
  "fingerprint": {
    "tests": [
      {
        "page": "first",
        "match": [
          {
            "text": "Policy Overview",
            "type": "startsWith",
            "isCaseSensitive": true
          },
          {
            "text": "Grand Total",
            "type": "startsWith",
            "isCaseSensitive": true
          }
        ]
      },
      {
        "page": "every",
        "match": [
          {
            "text": "Loss Experience Report",
            "type": "startsWith",
            "isCaseSensitive": true
          },
          {
            "text": "Claims Details",
            "type": "startsWith",
            "isCaseSensitive": true
          }
        ]
      }
    ]
  },
  "fields": [
    {
      "id": "_insured_raw",
      "method": {
        "id": "label",
        "position": "above"
      },
      "anchor": {
        "match": {
          "text": "policy overview",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "policy_start_date",
      "type": "date",
      "method": {
        "id": "label",
        "position": "right",
        "tiebreaker": "<"
      },
      "anchor": {
        "match": {
          "text": "policies incepting between",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "policy_end_date",
      "type": "date",
      "method": {
        "id": "label",
        "position": "right",
        "tiebreaker": ">"
      },
      "anchor": {
        "match": {
          "text": "policies incepting between",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "policy_premium",
      "type": "currency",
      "method": {
        "id": "passthrough"
      },
      "anchor": "not configured not found"
    },
    {
      "id": "open_claims",
      "type": "number",
      "method": {
        "id": "intersection",
        "verticalAnchor": {
          "match": {
            "text": "open",
            "type": "startsWith"
          }
        },
        "offsetX": 0.1
      },
      "anchor": {
        "match": {
          "text": "grand total",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "closed_claims",
      "type": "number",
      "method": {
        "id": "passthrough"
      },
      "anchor": "not found"
    },
    {
      "id": "total_claims",
      "type": "number",
      "method": {
        "id": "intersection",
        "verticalAnchor": {
          "match": {
            "text": "total",
            "type": "startsWith"
          }
        },
        "offsetX": 0.1
      },
      "anchor": {
        "match": {
          "text": "grand total",
          "type": "startsWith"
        }
      }
    }
  ],
  "computed_fields": [
    {
      "id": "insured",
      "method": {
        "id": "split",
        "source_id": "_insured_raw",
        "separator": "- ",
        "index": 1
      }
    },
    {
      "id": "policy_number",
      "method": {
        "id": "split",
        "source_id": "_insured_raw",
        "separator": " - ",
        "index": 0
      }
    },
    {
      "id": "hide_fields",
      "method": {
        "id": "suppressOutput",
        "source_ids": [
          "_insured_raw"
        ]
      }
    }
  ],
  "sections": [
    {
      "id": "claims_details",
      "range": {
        "anchor": {
          "start": {
            "text": "Claims Details",
            "type": "startsWith",
            "isCaseSensitive": true
          },
          "match": {
            "type": "startsWith",
            "text": "claim #"
          }
        }
      },
      "fields": [
        {
          "id": "claim_number",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "first"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "claim #"
            }
          }
        },
        {
          "id": "claim_type",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "first"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "claim status"
            }
          }
        },
        {
          "id": "loss_date",
          "type": "date",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "first"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "injury date"
            }
          }
        },
        {
          "id": "claim_date",
          "type": "date",
          "method": {
            "id": "row",
            "position": "right",
            "tiebreaker": "first"
          },
          "anchor": {
            "match": {
              "type": "startsWith",
              "text": "reported date"
            }
          }
        },
        {
          "id": "claim_amount_paid.total",
