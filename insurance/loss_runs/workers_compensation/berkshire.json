{
  "fingerprint": {
    "tests": [
      {
        "page": "first",
        "match": [
          {
            "text": "SUMMARY TOTALS BY POLICY YEAR",
            "type": "startsWith",
            "isCaseSensitive": true
          }
        ]
      },
      {
        "page": "every",
        "match": [
          {
            "text": "LOSS RUN REPORT",
            "type": "includes",
            "isCaseSensitive": true
          },
          {
            "text": "Valuation Date:",
            "type": "includes",
            "isCaseSensitive": true
          }
        ]
      }
    ]
  },
  "fields": [
    {
      "id": "insured",
      "method": {
        "id": "label",
        "position": "right"
      },
      "anchor": {
        "match": {
          "text": "insured:",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "policy_start_date",
      "type": "number",
      "method": {
        "id": "passthrough"
      },
      "anchor": {
        "start": "summary totals by policy year",
        "match": {
          "pattern": "^20\\d{2}$",
          "type": "regex"
        },
        "end": "grand total"
      }
    },
    {
      "id": "policy_end_date",
      "match": "last",
      "type": "number",
      "method": {
        "id": "passthrough"
      },
      "anchor": {
        "start": "summary totals by policy year",
        "match": {
          "pattern": "^20\\d{2}$",
          "type": "regex"
        },
        "end": "grand total"
      }
    },
    {
      "id": "policy_premium",
      "method": {
        "id": "passthrough"
      },
      "anchor": "not configured, not found"
    },
    {
      "id": "policy_number",
      "method": {
        "id": "label",
        "position": "right"
      },
      "anchor": {
        "match": {
          "text": "policy #:",
          "type": "startsWith"
        }
      }
    },
    {
      "id": "open_claims",
      "type": "number",
      "method": {
        "id": "passthrough"
      },
      "anchor": "not found on sample"
    },
    {
      "id": "closed_claims",
      "type": "number",
      "match": "last",
      "method": {
        "id": "passthrough"
      },
      "anchor": "not found on sample"
    },
    {
      "id": "total_claims",
      "type": "number",
      "method": {
        "id": "row",
        "position": "right",
        "tiebreaker": "first"
      },
      "anchor": {
        "match": {
          "text": "grand total",
          "type": "startsWith"
        }
      }
    }
  ],
