{
    "fingerprint": {
      "tests": [
        {
          "page": "every",
          "match": [
            {
              "text": "Master/Subsidiary Producer",
              "type": "equals",
              "isCaseSensitive": true
            },
            {
              "text": "Loss Run Report",
              "type": "equals",
              "isCaseSensitive": true
            },
            {
              "text": "PAC",
              "type": "equals",
              "isCaseSensitive": true
            }
          ]
        },
        {
          "page": "last",
          "match": [
            {
              "text": "Grand Total",
              "type": "equals",
              "isCaseSensitive": true
            }
          ]
        }
      ]
    },
