                },
                "anchor": "not configured, not found"
              },
              {
                "id": "claim_amount_incurred.indemnity",
                "type": "currency",
                "method": {
                  "id": "passthrough"
                },
                "anchor": "not configured, not found"
              },
              {
                "id": "claim_amount_incurred.expense",
                "type": "currency",
                "method": {
                  "id": "passthrough"
                },
                "anchor": "not configured, not found"
              },
              {
                "id": "accident_description",
                "method": {
                  "id": "label",
                  "position": "right"
                },
                "anchor": {
                  "start": {
                    "type": "regex",
                    "pattern": "^[A-Z]{2}[\\d]{7}"
                  },
                  "match": {
                    "type": "regex",
                    "pattern": "\\d{1,2}/\\d{1,2}/\\d{4}"
                  }
                }
              }
            ],
            "computed_fields": [
              {
                "id": "hide_fields",
                "method": {
                  "id": "suppressOutput",
                  "source_ids": [
                    "_claim_status_raw"
                  ]
                }
              },
              {
                "id": "claim_status",
                "method": {
                  "id": "split",
                  "source_id": "_claim_status_raw",
                  "separator": " ",
                  "index": 0
                }
              }
            ]
          }
        ]
      }
    ]
  }