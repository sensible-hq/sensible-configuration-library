                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^[A-Z]{2}[\\d]{7}"
                  }
                }
              },
              {
                "id": "claim_amount_reserves.total",
                "type": "currency",
                "method": {
                  "id": "passthrough"
                },
                "anchor": "not configured, not found"
              },
              {
