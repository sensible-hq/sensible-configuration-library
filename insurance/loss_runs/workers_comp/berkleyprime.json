{
    "fingerprint": {
      "tests": [
        {
          "page": "first",
          "match": [
            {
              "text": "Name:",
              "type": "equals",
              "isCaseSensitive": true
            },
            {
              "text": "Account Number:",
              "type": "equals",
              "isCaseSensitive": true
            },
            {
              "text": "Valued as of",
              "type": "startsWith",
              "isCaseSensitive": true
            }
          ]
        }
      ]
    },
    "fields": [
      {
        "id": "insured",
        "method": {
          "id": "row",
          "position": "right",
          "tiebreaker": "first"
        },
        "anchor": {
          "match": {
            "text": "name:",
            "type": "startsWith"
          }
        }
      }
    ],
    "sections": [
      {
        "id": "loss_run_policy",
        "range": {
          "anchor": {
            "match": {
              "pattern": "^[\\d]{7}-\\d$",
              "type": "regex"
            }
          },
          "stop": {
            "type": "any",
            "matches": [
              {
                "pattern": "^[\\d]{7}-\\d Total",
                "type": "regex"
              }
            ]
          },
          "requireStop": true
        },
        "fields": [
          {
            "id": "policy_start_date",
            "type": "date",
            "method": {
              "id": "row",
              "position": "right",
              "tiebreaker": "first"
            },
            "anchor": {
              "match": {
                "pattern": "^[\\d]{7}-",
                "type": "regex"
              },
              "end": "$"
            }
          },
          {
            "id": "policy_end_date",
            "type": "date",
            "method": {
              "id": "region",
              "start": "left",
              "width": 0.7,
              "height": 0.2,
              "offsetX": 0.7,
              "offsetY": 0.1
            },
            "anchor": {
              "match": {
                "pattern": "^[\\d]{7}-",
                "type": "regex"
              },
              "end": "$"
            }
          },
          {
            "id": "policy_premium",
            "method": {
              "id": "passthrough"
            },
            "anchor": "not configured, not found"
          },
          {
            "id": "policy_number",
            "method": {
              "id": "passthrough"
            },
            "anchor": {
              "match": {
                "pattern": "^[\\d]{7}-",
                "type": "regex"
              },
              "end": "$"
            }
          },
          {
            "id": "open_claims",
            "type": "number",
            "method": {
              "id": "passthrough"
            },
            "anchor": "not configured, not found"
          },
          {
            "id": "closed_claims",
            "type": "number",
            "method": {
              "id": "passthrough"
            },
            "anchor": "not configured, not found"
          },
          {
            "id": "total_claims",
            "type": "number",
            "method": {
              "id": "passthrough"
            },
            "anchor": "not configured, not found"
          }
        ],
        "sections": [
          {
            "id": "claims_details",
            "range": {
              "anchor": {
                "start": {
                  "pattern": "^[\\d]{7}-",
                  "type": "regex"
                },
                "match": {
                  "type": "regex",
                  "pattern": "^[A-Z]{2}[\\d]{7}"
                }
              },
              "stop": {
                "type": "endsWith",
                "text": "total"
              }
            },
            "fields": [
              {
                "id": "claim_number",
                "method": {
                  "id": "passthrough"
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^[A-Z]{2}[\\d]{7}"
                  }
                }
              },
              {
                "id": "claim_type",
                "match": "all",
                "method": {
                  "id": "row",
                  "position": "right",
                  "tiebreaker": "second",
                  "wordFilters": [
                    "close"
                  ]
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^[A-Z]{2}[\\d]{7}"
                  }
                }
              },
              {
                "id": "loss_date",
                "type": "date",
                "method": {
                  "id": "row",
                  "position": "right",
                  "tiebreaker": "first"
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^[A-Z]{2}[\\d]{7}"
                  }
                }
              },
              {
                "id": "claim_date",
                "type": "date",
                "method": {
                  "id": "passthrough"
                },
                "anchor": "not configured, not found"
              },
              {
                "id": "claimant",
