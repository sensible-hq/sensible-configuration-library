{
    "fingerprint": {
      "tests": [
        {
          "page": "every",
          "match": [
            {
              "text": "Loss Run Report",
              "type": "equals",
              "isCaseSensitive": true
            },
            {
              "text": "Insured:",
              "type": "equals",
              "isCaseSensitive": true
            },
            {
              "text": "Historical Period:",
              "type": "equals",
              "isCaseSensitive": true
            }
          ]
        },
        {
          "page": "last",
          "match": {
            "type": "any",
            "matches": [
              {
                "text": "TOTALS:",
                "type": "equals",
                "isCaseSensitive": true
              },
              {
                "type": "startsWith",
                "text": "No claims found"
              }
            ]
          }
        }
      ]
    },
    "preprocessors": [
      {
        "type": "ocr",
        "match": "page: 1",
        "matchAll": true,
        "pageOffset": 0
      }
    ],
    "sections": [
      {
        "id": "loss_run_policy",
        "range": {
          "anchor": {
            "match": {
              "text": "Policy Number:",
              "type": "equals"
            }
          },
          "stop": {
            "type": "any",
            "matches": [
              {
                "type": "equals",
                "text": "TOTALS:",
                "isCaseSensitive": true
              },
              {
                "type": "startsWith",
                "text": "No claims found"
              }
            ]
          },
          "requireStop": true
        },
        "fields": [
          {
            "id": "insured",
            "method": {
              "id": "row",
              "position": "right",
              "tiebreaker": "first"
            },
            "anchor": {
              "match": {
                "text": "insured:",
                "type": "startsWith"
              }
            }
          },
          {
            "id": "policy_start_date",
            "type": "date",
            "method": {
              "id": "label",
              "position": "right",
              "tiebreaker": "<"
            },
            "anchor": {
              "match": {
                "text": "Historical Period:",
                "type": "startsWith"
              }
            }
          },
          {
            "id": "policy_start_date",
            "type": "date",
            "method": {
              "id": "row",
              "position": "right",
              "tiebreaker": "first"
            },
            "anchor": {
              "match": {
                "text": "Historical Period:",
                "type": "startsWith"
              }
            }
          },
          {
            "id": "policy_end_date",
            "type": "date",
            "method": {
              "id": "label",
              "position": "right",
              "tiebreaker": ">"
            },
            "anchor": {
              "match": {
                "text": "Historical Period:",
                "type": "startsWith"
              }
            }
          },
          {
            "id": "policy_end_date",
            "type": "date",
            "method": {
              "id": "row",
              "position": "right",
              "tiebreaker": "second"
            },
            "anchor": {
              "match": {
                "text": "Historical Period:",
                "type": "startsWith"
              }
            }
          },
          {
            "id": "policy_premium",
            "method": {
              "id": "passthrough"
            },
            "anchor": "not configured, not found"
          },
          {
            "id": "policy_number",
            "method": {
              "id": "row",
              "position": "right",
              "tiebreaker": "first"
            },
            "anchor": {
              "match": {
                "type": "startsWith",
                "text": "policy number"
              }
            }
          },
          {
            "id": "open_claims",
            "type": "number",
            "method": {
              "id": "passthrough"
            },
            "anchor": "not configured, not found"
          },
          {
            "id": "closed_claims",
            "type": "number",
            "method": {
              "id": "passthrough"
            },
            "anchor": "not configured, not found"
          },
          {
            "id": "total_claims",
            "type": "number",
            "method": {
              "id": "passthrough"
            },
            "anchor": "not configured, not found"
          }
        ],
        "sections": [
          {
            "id": "claims_details",
            "range": {
              "anchor": {
                "match": {
                  "type": "regex",
                  "pattern": "^\\d{12}"
                }
              }
            },
            "fields": [
              {
                "id": "claim_number",
                "method": {
                  "id": "passthrough"
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^\\d{12}"
                  }
                }
              },
              {
                "id": "_loss_ded_raw",
                "match": "all",
                "method": {
                  "id": "regex",
                  "pattern": "[A-Z]{2}/[\\d.]+"
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^\\d{12}"
                  }
                }
              },
              {
                "id": "loss_date",
                "type": "date",
                "method": {
                  "id": "row",
                  "position": "right",
                  "tiebreaker": "first"
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "[A-Z]{2}/[\\d]+"
                  }
                }
              },
              {
                "id": "claim_date",
                "type": "date",
                "method": {
                  "id": "row",
                  "position": "right",
                  "tiebreaker": "first"
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^[A-Z]{3}\\d{7}$"
                  }
                }
              },
              {
                "id": "claimant",
                "match": "all",
                "method": {
                  "id": "label",
                  "position": "below"
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^\\d{5}$"
                  }
                }
              },
              {
                "id": "claim_status",
                "match": "all",
                "method": {
                  "id": "row",
                  "position": "right",
                  "tiebreaker": "first"
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^\\d{12}"
                  }
                }
              },
              {
                "id": "claim_amount_paid.total",
                "type": "currency",
                "method": {
                  "id": "passthrough"
                },
                "anchor": "not configured, not found"
              },
              {
                "id": "claim_amount_paid.medical",
                "type": "currency",
                "method": {
                  "id": "passthrough"
                },
                "anchor": "not configured, not found"
              },
              {
                "id": "claim_amount_paid.indemnity",
                "match": "all",
                "type": "currency",
                "method": {
                  "id": "row",
                  "position": "right",
                  "tiebreaker": "second"
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^\\d{12}"
                  }
                }
              },
              {
                "id": "claim_amount_paid.expense",
                "match": "all",
                "type": "currency",
                "method": {
                  "id": "row",
                  "position": "right",
                  "tiebreaker": "third"
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^\\d{12}"
                  }
                }
              },
              {
                "id": "claim_amount_reserves.total",
                "match": "all",
                "type": "currency",
                "method": {
                  "id": "row",
                  "position": "right",
                  "tiebreaker": "first"
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^\\d{12}"
                  }
                }
              },
              {
                "id": "claim_amount_reserves.indemnity",
                "type": "currency",
                "method": {
                  "id": "passthrough"
                },
                "anchor": "not configured, not found"
              },
              {
                "id": "claim_amount_reserves.expense",
                "type": "currency",
                "method": {
                  "id": "passthrough"
                },
                "anchor": "not configured, not found"
              },
              {
                "id": "claim_amount_recoveries.total",
                "match": "all",
                "type": "currency",
                "method": {
                  "id": "region",
                  "start": "above",
                  "width": 0.9,
                  "height": 0.15,
                  "offsetX": 8,
                  "offsetY": 0
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^\\d{12}"
                  }
                }
              },
              {
                "id": "claim_amount_incurred.total",
                "match": "all",
                "type": "currency",
                "method": {
                  "id": "row",
                  "position": "right",
                  "tiebreaker": "last"
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^\\d{12}"
                  }
                }
              },
              {
                "id": "claim_amount_incurred.medical",
                "type": "currency",
                "method": {
                  "id": "passthrough"
                },
                "anchor": "not configured, not found"
              },
              {
                "id": "claim_amount_incurred.indemnity",
                "match": "all",
                "type": "currency",
                "method": {
                  "id": "region",
                  "start": "above",
                  "width": 0.9,
                  "height": 0.15,
                  "offsetX": 6,
                  "offsetY": 0
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^\\d{12}"
                  }
                }
              },
              {
                "id": "claim_amount_incurred.expense",
                "match": "all",
                "type": "currency",
                "method": {
                  "id": "region",
                  "start": "above",
                  "width": 0.9,
                  "height": 0.15,
                  "offsetX": 7,
                  "offsetY": 0
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^\\d{12}"
                  }
                }
              },
              {
                "id": "accident_description",
                "type": "paragraph",
                "method": {
                  "id": "region",
                  "start": "left",
                  "width": 1.7,
                  "height": 0.15,
                  "offsetX": 0,
                  "offsetY": 0.3
                },
                "anchor": {
                  "match": {
                    "type": "regex",
                    "pattern": "^[A-Z]{3}\\d{7}$"
                  }
                }
              }
            ],
            "computed_fields": [
              {
                "id": "hide_fields",
                "method": {
                  "id": "suppressOutput",
                  "source_ids": [
                    "_loss_ded_raw"
                  ]
                }
              },
              {
                "id": "claim_type",
                "method": {
                  "id": "split",
                  "source_id": "_loss_ded_raw",
                  "separator": "/",
                  "index": 0
                }
              }
            ]
          }
        ]
      }
    ],
    "fields": []
  }